{
  "name": "github-projects-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsc && vite",
    "build": "tsc && vite build",
    "typecheck": "tsc",
    "test": "tsc && playwright test",
    "test:ui": "tsc && playwright test --ui",
    "test:headed": "tsc && playwright test --headed",
    "test:debug": "tsc && playwright test --debug",
    "test:chrome": "tsc && playwright test --project=chromium",
    "test:firefox": "tsc && playwright test --project=firefox",
    "test:safari": "tsc && playwright test --project=webkit"
  },
  "devDependencies": {
    "svelte": "^5.23.1",
    "vite": "^6.3.1",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@playwright/test": "^1.55.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "autoprefixer": "^10.4.21",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.4",
    "dayjs": "^1.11.13",
    "liqe": "^3.8.3",
    "lucide-svelte": "^0.503.0"
  }
}
